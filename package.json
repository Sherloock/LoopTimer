{
	"name": "workout-timer",
	"version": "0.1.0",
	"private": true,
	"//": "DOCUMENTATION: See package.json.scripts.md for detailed script documentation. Keep in sync with README.md.",
	"scripts": {
		"// [DEVELOPMENT]": "==============================================",
		"postinstall": "prisma generate",
		"nvm-use": "nvm use 20.19.0",
		"npmi:clean": "rimraf node_modules package-lock.json && npm i",
		"dev": "npm run nvm-use && (npm run dev:db || true) && npx next dev -p 3002",
		"dev:turbo": "npm run nvm-use && (npm run dev:db || true) && npx next dev --turbopack -p 3002",
		"dev:stop": "node scripts/stop-dev-server.js",
		"dev:db": "docker compose up -d db || echo Database not started (docker not running?)",
		"// [BUILD & PRODUCTION]": "==============================================",
		"build": "prisma generate && npm run format && next build",
		"vercel-build": "prisma migrate deploy && prisma db seed && next build",
		"start": "next start",
		"// [TESTING]": "==============================================",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:install": "playwright install",
		"test:db": "jest db-connection",
		"// [CODE QUALITY]": "==============================================",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"lint-eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"remove-unused": "npx eslint . --fix --quiet --rule \"unused-imports/no-unused-imports:error\"",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"sync:ai-rules": "node scripts/sync-ai-rules.js",
		"// [DATABASE - PRISMA DEV]": "==============================================",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:pull": "prisma db pull",
		"db:migrate": "prisma migrate dev",
		"db:migrate:create": "prisma migrate dev --create-only",
		"db:migrate:reset": "prisma migrate reset",
		"db:validate": "prisma validate",
		"db:format": "prisma format",
		"db:studio": "prisma studio",
		"db:verify": "tsx scripts/verify-db.mjs",
		"// [DATABASE - MIGRATIONS PROD]": "==============================================",
		"db:migrate:deploy": "prisma migrate deploy",
		"db:migrate:deploy:prod": "node scripts/prisma-with-env.mjs .env.prod migrate deploy",
		"// [DATABASE - SEEDING]": "==============================================",
		"db:seed": "tsx --env-file=.env prisma/seed.ts",
		"db:seed:local": "tsx --env-file=.env.local prisma/seed.ts",
		"db:seed:dev": "tsx --env-file=.env.development.local prisma/seed.ts",
		"db:seed:prod": "tsx --env-file=.env.prod prisma/seed.ts",
		"// [DATABASE - DOCKER]": "==============================================",
		"db:up": "docker compose up -d db",
		"db:down": "docker compose down",
		"db:ps": "docker compose ps",
		"db:start": "docker start timer-postgres",
		"db:stop": "docker stop timer-postgres",
		"db:logs": "docker logs timer-postgres",
		"db:restart": "docker restart timer-postgres",
		"// [DATABASE - ALIASES]": "==============================================",
		"prisma:migrate": "npm run db:migrate",
		"prisma:studio": "npm run db:studio",
		"prisma:seed": "npm run db:seed",
		"// [VERCEL DEPLOYMENT]": "==============================================",
		"vercel:deploy": "vercel",
		"vercel:deploy:prod": "vercel --prod",
		"vercel:env:pull": "vercel env pull .env.local",
		"vercel:env:pull:prod": "vercel env pull .env.prod --environment=production",
		"vercel:env:add": "vercel env add",
		"vercel:inspect": "vercel inspect",
		"vercel:inspect:prod": "vercel inspect https://timer.vercel.app/",
		"vercel:logs": "vercel logs",
		"vercel:logs:prod": "vercel logs https://timer.vercel.app/",
		"// [DEPLOYMENT ALIASES]": "==============================================",
		"deploy": "npm run vercel:deploy:prod",
		"vercel": "npm run vercel:deploy",
		"vercel:prod": "npm run vercel:deploy:prod",
		"prepare": "husky"
	},
	"dependencies": {
		"@clerk/nextjs": "^6.36.8",
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@prisma/client": "^6.19.2",
		"@radix-ui/react-accordion": "^1.2.3",
		"@radix-ui/react-alert-dialog": "^1.1.6",
		"@radix-ui/react-aspect-ratio": "^1.1.2",
		"@radix-ui/react-avatar": "^1.1.3",
		"@radix-ui/react-checkbox": "^1.3.3",
		"@radix-ui/react-collapsible": "^1.1.3",
		"@radix-ui/react-context-menu": "^2.2.6",
		"@radix-ui/react-dialog": "^1.1.6",
		"@radix-ui/react-dropdown-menu": "^2.1.6",
		"@radix-ui/react-hover-card": "^1.1.6",
		"@radix-ui/react-label": "^2.1.2",
		"@radix-ui/react-menubar": "^1.1.6",
		"@radix-ui/react-navigation-menu": "^1.2.5",
		"@radix-ui/react-popover": "^1.1.6",
		"@radix-ui/react-progress": "^1.1.0",
		"@radix-ui/react-radio-group": "^1.2.3",
		"@radix-ui/react-scroll-area": "^1.2.3",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-separator": "^1.1.2",
		"@radix-ui/react-slider": "^1.2.3",
		"@radix-ui/react-slot": "^1.1.2",
		"@radix-ui/react-switch": "^1.1.3",
		"@radix-ui/react-tabs": "^1.1.3",
		"@radix-ui/react-toast": "^1.2.6",
		"@radix-ui/react-toggle": "^1.1.2",
		"@radix-ui/react-toggle-group": "^1.1.2",
		"@radix-ui/react-tooltip": "^1.1.8",
		"@tanstack/react-query": "^5.67.2",
		"@tanstack/react-query-devtools": "^5.67.2",
		"autoprefixer": "^10.4.21",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.0.0",
		"date-fns": "^3.6.0",
		"dotenv": "^17.2.3",
		"file-saver": "^2.0.5",
		"groq-sdk": "^0.37.0",
		"jsqr": "^1.4.0",
		"lucide-react": "^0.479.0",
		"next": "^15.5.9",
		"next-themes": "^0.4.5",
		"nextjs-toploader": "^3.7.15",
		"postcss": "^8.5.3",
		"qrcode": "^1.5.4",
		"react": "^18.2.0",
		"react-colorful": "^5.6.1",
		"react-countup": "^6.5.3",
		"react-day-picker": "^8.10.1",
		"react-dom": "^18.2.0",
		"rimraf": "^6.0.1",
		"sonner": "^2.0.1",
		"tailwind-merge": "^2.6.0",
		"tailwindcss-animate": "^1.0.7",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@playwright/test": "^1.57.0",
		"@shadcn/ui": "^0.0.4",
		"@tailwindcss/postcss": "^4.0.12",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.2",
		"@testing-library/user-event": "^14.6.1",
		"@types/file-saver": "^2.0.7",
		"@types/jest": "^30.0.0",
		"@types/node": "^20.17.24",
		"@types/qrcode": "^1.5.5",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"eslint": "^9",
		"eslint-config-next": "^15.5.9",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.5.5",
		"husky": "^9.1.7",
		"jest": "^30.2.0",
		"jest-environment-jsdom": "^30.2.0",
		"lint-staged": "^16.2.7",
		"postcss": "^8",
		"prettier": "^3.4.2",
		"prettier-plugin-prisma": "^5.0.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"prisma": "^6.19.2",
		"shadcn": "^3.7.0",
		"sharp": "^0.34.5",
		"tailwindcss": "^3.4.1",
		"ts-jest": "^29.4.6",
		"tsx": "^4.19.3",
		"typescript": "^5.8.2",
		"vercel": "^50.4.11"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,css,md}": [
			"prettier --write"
		]
	}
}
